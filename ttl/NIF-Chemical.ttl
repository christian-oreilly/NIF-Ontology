NIFCHEM:DB00813 a owl:Class ;
    rdfs:label "Fentanyl"^^xsd:string ;
    OBOANN:synonym "Actiq"^^xsd:string,
        "Duragesic"^^xsd:string,
        "Duragesic-100"^^xsd:string,
        "Durogesic"^^xsd:string,
