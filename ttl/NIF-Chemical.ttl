        "Fentanila (INN-Spanish)"^^xsd:string,
        "fentanyl"^^xsd:string,
        "Fentanyl citrate"^^xsd:string,
        "Fentanylum (INN-Latin)"^^xsd:string,
        "Nasalfent"^^xsd:string,
        "Pentanyl"^^xsd:string,
