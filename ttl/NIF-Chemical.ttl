CHEBI:27958 a owl:Class ;
    rdfs:label "Cocaine"^^xsd:string ;
    OBOANN:synonym "Benzoylethylecgonine"^^xsd:string,
        "Benzoylmethylecgonine"^^xsd:string,
        "Beta-Cocain"^^xsd:string,
        "Cocain"^^xsd:string,
        "Cocaina"^^xsd:string,
        "Cocaine hydrochloride"^^xsd:string,
        "Cocaine-M"^^xsd:string,
        "D-pseudococaine"^^xsd:string,
