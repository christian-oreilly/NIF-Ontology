NIFUNCL:CHEBI_24848 a owl:Class ;
    rdfs:label "Inositol"^^xsd:string ;
    skos:definition "Is Cyclohexane-1,2,3,4,5,6-hexol, a chemical compound with formula C6H12O6 or (-CHOH-)6, a six-fold alcohol (polyol) of cyclohexane. It exists in nine possible stereoisomers, of which the most prominent form, widely occuring in nature, is cis-1,2,3,5-trans-4,6-cyclohexanehexol, or myo-inositol. Other naturally occurring isomers (though in minimal quantities) are scyllo-, chiro-, muco-, and neo-inositol. The other possible isomers are allo-, epi-, and cis-inositol. (from Wikipedia)"^^xsd:string ;
    rdfs:subClassOf NIFUNCL:nlx_200905_396 ;
    OBOANN:createdDate "August 21, 2009"^^xsd:string .

NIFUNCL:GO_0006954 a owl:Class ;
    rdfs:label "Inflammatory response"^^xsd:string ;
    skos:definition "The immediate defensive reaction (by vertebrate tissue) to infection or injury caused by chemical or physical agents. The process is characterized by local vasodilation, extravasation of plasma into intercellular spaces and accumulation of white blood cells and macrophages."^^xsd:string ;
    rdfs:subClassOf NIFUNCL:nlx_200905_396 ;
    rdfs:comment "Related to: Inflammation"^^xsd:string ;
    OBOANN:createdDate "August 21, 2009"^^xsd:string .

NIFUNCL:nlx_0906_MP_0001399 a owl:Class ;
    rdfs:label "Hyperactivity"^^xsd:string ;
    OBOANN:synonym "increased locomotor activity"^^xsd:string ;
    skos:definition "General restlessness or excessive movement; more frequent movement from one place to another or having an increased state of activity."^^xsd:string ;
    rdfs:subClassOf NIFUNCL:nlx_200905_396 ;
    OBOANN:createdDate "June 24, 2009"^^xsd:string ;
    OBOANN:externalSourceId "MP_0001399"^^xsd:string .

NIFUNCL:nlx_0906_MP_0005039 a owl:Class ;
    rdfs:label "Hypoxia"^^xsd:string ;
    OBOANN:synonym "Decreased partial O2 level"^^xsd:string ;
    skos:definition "reduced concentration of O2 in the blood, alveoli or other tissues resulting in the decreased pressure of this component of body gases (Mammalian Phenotype Ontology)"^^xsd:string ;
    rdfs:subClassOf NIFUNCL:nlx_200905_396 ;
    OBOANN:createdDate "June 24, 2009"^^xsd:string ;
    OBOANN:externalSourceId "MP_0005039"^^xsd:string ;
    dc:contributor "Maryann Martone"^^xsd:string .

NIFUNCL:nlx_0906_MP_0005407 a owl:Class ;
    rdfs:label "Hyperalgesia"^^xsd:string ;
    OBOANN:synonym "decreased pain threshold"^^xsd:string ;
    skos:definition "increased sensation to painful stimuli that may be due to damage to soft tissue containing nociceptors or injury to a peripheral nerve; it can be primary (at the site of the injury) or secondary (in the surrounding undamaged area)(Mammalian phenotype ontology)"^^xsd:string ;
    rdfs:subClassOf NIFUNCL:nlx_200905_396 ;
    OBOANN:createdDate "June 24, 2009"^^xsd:string ;
    OBOANN:externalSourceId "MP_0005407"^^xsd:string .

NIFUNCL:nlx_090801 a owl:Class ;
    rdfs:label "Impulsivity"^^xsd:string ;
    skos:definition "A behavioral trait where an individual acts on impulse rather than thought. Although part of the normal behavior, impulsivity also plays a role in many mental illnesses (adapted from Wikipedia)"^^xsd:string ;
    rdfs:subClassOf NIFUNCL:nlx_200905_396 ;
    OBOANN:createdDate "August 20, 2009"^^xsd:string .

NIFUNCL:nlx_090802 a owl:Class ;
    rdfs:label "Lesion"^^xsd:string ;
    skos:definition "A localized pathological or traumatic structural change, damage, deformity, or discontinuity of tissue, organ, or body part (adapted from NCI Metathesaurus)"^^xsd:string ;
    rdfs:subClassOf NIFUNCL:nlx_200905_396 ;
    OBOANN:createdDate "August 21, 2009"^^xsd:string ;
    OBOANN:UmlsCui "C0221198"^^xsd:string .

NIFUNCL:nlx_090803 a owl:Class ;
    rdfs:label "Injury"^^xsd:string ;
